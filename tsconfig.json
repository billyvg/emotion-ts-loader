{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": false,
    "declaration": false,
    "declarationMap": false,
    "downlevelIteration": true,
    "inlineSources": true,
    "importHelpers": true,
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "strictBindCallApply": false,
    "experimentalDecorators": true,

    "esModuleInterop": true,
    "outDir": "./dist",
    "types": [],
    "jsx": "react"
  },
  "files": ["./src/client/client.tsx"]
}
